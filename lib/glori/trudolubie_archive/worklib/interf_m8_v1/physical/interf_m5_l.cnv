#This is the Layer Conversion File used for
#importing DXF data into Allegro/APD.

#CLASS!            SUBCLASS!                 DXF_LAYER!

BOARD GEOMETRY!
                   SOLDERMASK_TOP!           BOARD_GEOMETRY_SOLDERMASK_TOP!
                   OUTLINE!                  BOARD_GEOMETRY_OUTLINE!

ETCH!
                   TOP!                      ETCH_TOP!
                   GND!                      ETCH_GND!
                   PWR!                      ETCH_PWR!
                   BOTTOM!                   ETCH_BOTTOM!

MANUFACTURING!
                   NO_PROBE_TOP!             MANUFACTURING_NO_PROBE_TOP!
                   NCDRILL_FIGURE!           MANUFACTURING_NCDRILL_FIGURE!
                   NCLEGEND-1-4!             MANUFACTURING_NCLEGEND-1-4!

PACKAGE GEOMETRY!
                   SOLDERMASK_TOP!           PACKAGE_GEOMETRY_SOLDERMASK_TOP!
                   ASSEMBLY_TOP!             PACKAGE_GEOMETRY_ASSEMBLY_TOP!

PACKAGE KEEPIN!
                   ALL!                      PACKAGE_KEEPIN_ALL!

PACKAGE KEEPOUT!
                   BOTTOM!                   PACKAGE_KEEPOUT_BOTTOM!
                   TOP!                      PACKAGE_KEEPOUT_TOP!
                   ALL!                      PACKAGE_KEEPOUT_ALL!

PIN!
                   SOLDERMASK_TOP!           PIN_SOLDERMASK_TOP!
                   TOP!                      PIN_TOP!
                   GND!                      PIN_GND!
                   PWR!                      PIN_PWR!
                   BOTTOM!                   PIN_BOTTOM!

REF DES!
                   SILKSCREEN_TOP!           REF_DES_SILKSCREEN_TOP!

ROUTE KEEPIN!
                   ALL!                      ROUTE_KEEPIN_ALL!

ROUTE KEEPOUT!
                   ALL!                      ROUTE_KEEPOUT_ALL!
                   TOP!                      ROUTE_KEEPOUT_TOP!
                   GND!                      ROUTE_KEEPOUT_GND!
                   PWR!                      ROUTE_KEEPOUT_PWR!
                   BOTTOM!                   ROUTE_KEEPOUT_BOTTOM!

VIA CLASS!
                   TOP!                      VIA_CLASS_TOP!
                   GND!                      VIA_CLASS_GND!
                   PWR!                      VIA_CLASS_PWR!
                   BOTTOM!                   VIA_CLASS_BOTTOM!

VIA KEEPOUT!
                   ALL!                      VIA_KEEPOUT_ALL!
                   TOP!                      VIA_KEEPOUT_TOP!
                   GND!                      VIA_KEEPOUT_GND!
                   PWR!                      VIA_KEEPOUT_PWR!
                   BOTTOM!                   VIA_KEEPOUT_BOTTOM!

#END